@ManagedBean(name = "gamesView")
@ViewScoped
public class GamesView implements Serializable{
	
    private static final long serialVersionUID = 1L;
    private List<Game> games;
    
    @PostConstruct
    public void init() {
        cars = service.createCars(100);
    }
}
