package dataAccessLayer;

import java.util.Comparator;

import model.Player;

public class RatingComparator implements Comparator {
	
	 @Override
	    public int compare(Player x, Player y){
	        if(x.dist<y.dist){
	            return -1;
	        }
	        if(x.dist>y.dist){
	            return 1;
	        }
	        return 0;
	    }

}
