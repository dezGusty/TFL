package dataAccessLayer;

import java.util.Comparator;

import model.Player;
import model.PlayerRating;

public class PlayerRatingComparator implements Comparator<PlayerRating> {

	@Override
	public int compare(PlayerRating arg0, PlayerRating arg1) {
		if(arg0.getDate().after(arg1.getDate()))
		{
			return 1;
		}
		return 0;
	}

}
